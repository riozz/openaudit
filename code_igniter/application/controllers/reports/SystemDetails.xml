		<report_name>System Details</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.id AS `system.id`, system.icon AS `system.icon`, system.type AS `system.type`, system.name AS `system.name`, system.domain AS `system.domain`, system.ip AS `system.ip`, system.os_name AS `system.os_name`, system.class AS `system.class`, system.owner AS `system.owner`, system.model AS `system.model`, system.first_seen as `system.first_seen`, system.last_seen as `system.last_seen`, system.last_seen_by AS `system.last_seen_by`, system.os_installation_date AS `system.os_installation_date`, system.manufacturer AS `system.manufacturer`, system.serial AS `system.serial`, system.form_factor AS `system.form_factor`, windows.client_site_name AS `windows.client_site_name`,  windows.user_name AS `windows.user_name`, oa_location.name AS `oa_location.name`, windows.active_directory_ou AS `windows.active_directory_ou` FROM system LEFT JOIN oa_location ON (system.location_id = oa_location.id) LEFT JOIN windows ON (system.id = windows.system_id AND windows.current = 'y') LEFT JOIN oa_group_sys ON (oa_group_sys.system_id = system.id) WHERE oa_group_sys.group_id = @group ORDER BY system.name]]></report_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>0</report_sort_column>
