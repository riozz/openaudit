<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Specific Service</report_name>
		<report_display_in_menu>n</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.id AS `system.id`, system.name AS `system.name`, service.name AS `service.name`, service.description AS `service.description`, service.state AS `service.state`, service.start_mode AS `service.start_mode` FROM system LEFT JOIN oa_group_sys ON (oa_group_sys.system_id = system.id) LEFT JOIN service ON (system.id = service.system_id AND service.current = 'y') WHERE oa_group_sys.group_id = @group AND service.name = (SELECT name FROM service WHERE id = ? LIMIT 1)]]></report_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>0</report_sort_column>
