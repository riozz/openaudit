<report>
	<details>
		<report_name>Device Details</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.id AS `system.id`, system.icon AS `system.icon`, system.type AS `system.type`, system.name AS `system.name`, system.domain AS `system.domain`, system.ip AS `system.ip`, system.first_seen AS `system.first_seen`, system.last_seen AS `system.last_seen`, system.last_seen_by AS `system.last_seen_by`, system.model AS `system.model`, system.manufacturer AS `system.manufacturer`, system.serial AS `system.serial`, system.owner AS `system.owner`, oa_location.name as `oa_location.name`, system.os_family AS `system.os_family`, oa_org.name as `oa_org.name` FROM system LEFT JOIN oa_location ON system.location_id = oa_location.id LEFT JOIN oa_group_sys ON (oa_group_sys.system_id = system.id) LEFT JOIN oa_org ON system.org_id = oa_org.id WHERE oa_group_sys.group_id = @group GROUP BY system.id ORDER BY system.name]]></report_sql>
		<report_view_file>v_report</report_view_file>
