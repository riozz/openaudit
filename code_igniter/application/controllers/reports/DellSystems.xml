<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Dell Systems</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.id AS `system.id`, system.icon AS `system.icon`, system.type AS `system.type`, system.name AS `system.name`, system.domain AS `system.domain`, system.ip AS `system.ip`, system.model AS `system.model`, system.serial AS `system.serial` FROM system LEFT JOIN oa_group_sys ON (oa_group_sys.system_id = system.id) WHERE oa_group_sys.group_id = @group AND system.manufacturer LIKE '%Dell%' GROUP BY system.id ORDER BY system.name]]></report_sql>
		<report_display_sql></report_display_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>0</report_sort_column>
		<report_description>Devices manufactured by Dell.</report_description>
	</details>
