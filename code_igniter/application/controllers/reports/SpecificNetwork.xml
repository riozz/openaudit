<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Specific Network</report_name>
		<report_display_in_menu>n</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.id AS `system.id`, system.icon AS `system.icon`, system.type AS `system.type`, system.name AS `system.name`, system.domain AS `system.domain`, system.description AS `system.description`, system.os_family AS `system.os_family`, ip.ip AS `ip.ip`, ip.network AS `ip.network` FROM system LEFT JOIN oa_group_sys ON (oa_group_sys.system_id = system.id) LEFT JOIN ip ON (system.id = ip.system_id AND ip.current = 'y') WHERE oa_group_sys.group_id = @group AND ip.network = (SELECT network FROM ip WHERE id = ? LIMIT 1)]]></report_sql>
		<report_view_file>v_report</report_view_file>
