<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Changes - Software</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.id AS `system.id`, system.icon AS `system.icon`, system.type AS `system.type`, system.name AS `system.name`, system.domain AS `system.domain`, system.ip AS `system.ip`, change_log.timestamp AS `change_log.timestamp`, change_log.db_table AS `change_log.db_table`, change_log.db_action AS `change_log.db_action`, change_log.details AS `change_log.details`, change_log.id AS `change_log.id` FROM change_log LEFT JOIN system ON (change_log.system_id = system.id) LEFT JOIN oa_group_sys ON (oa_group_sys.system_id = system.id) WHERE oa_group_sys.group_id = @group AND change_log.ack_time = '0000-00-00 00:00:00' AND change_log.db_table in ('service', 'server', 'server_item', 'software', 'software_key') ORDER BY system.name, change_log.id]]></report_sql>
		<report_view_file>v_report_alerts</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>0</report_sort_column>
		<report_description>Any changes in the tables 'service', 'server', 'server_item', 'software' and 'software_key'.</report_description>
	</details>
