<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Workstation Details</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.id AS `system.id`, system.icon AS `system.icon`, system.type AS `system.type`, system.name AS `system.name`, system.domain AS `system.domain`, system.ip AS `system.ip`, system.manufacturer AS `system.manufacturer`, system.model AS `system.model`, system.serial AS `system.serial`, system.last_seen AS `system.last_seen`, windows.user_name AS `windows.user_name`, oa_location.name AS `oa_location.name`, oa_org.name as `oa_org.name` FROM system LEFT JOIN oa_location ON (system.location_id = oa_location.id) LEFT JOIN oa_org ON (system.org_id = oa_org.id) LEFT JOIN oa_group_sys ON (oa_group_sys.system_id = system.id) LEFT JOIN windows ON (system.id = windows.system_id AND windows.current = 'y') WHERE oa_group_sys.group_id = @group GROUP BY system.id ORDER BY system.name]]></report_sql>
		<report_display_sql></report_display_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>0</report_sort_column>
		<report_description>Device details - name, description, last audited date, owner, Windows user, location, manufacturer, model, serial.</report_description>
