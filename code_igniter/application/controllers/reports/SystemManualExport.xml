<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>System Manual Export</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.id AS `system.id`, system.icon AS `system.icon`, system.type AS `system.type`, system.name AS `system.name`, system.domain AS `system.domain`, system.ip AS `system.ip`, system.vm_group AS `system.vm_group`, system.owner AS `system.owner`, system.description AS `system.description`, system.function AS `system.function`, system.environment AS `system.environment`, system.org_id AS `system.org_id`, system.location_id AS `system.location_id`, oa_location.name AS `oa_location.name` FROM system LEFT JOIN oa_group_sys ON (oa_group_sys.system_id = system.id) LEFT JOIN oa_location ON (system.location_id = oa_location.id) WHERE oa_group_sys.group_id = @group GROUP BY system.name ORDER BY system.name]]></report_sql>
		<report_display_sql></report_display_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
