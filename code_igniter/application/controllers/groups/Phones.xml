<group>
	<details>
		<group_name>Phones</group_name>
		<group_padded_name></group_padded_name>
		<group_dynamic_select><![CDATA[SELECT distinct(system.id) FROM system WHERE system.status = 'production' and type LIKE '%phone']]></group_dynamic_select>
		<group_parent>1</group_parent>
		<group_category>device</group_category>
		<group_icon>cell_phone</group_icon>
		<group_description>Any items that have their status attribute set to 'production' and have their type attribute set to 'phone', 'smart phone' or 'ip phone'.</group_description>
		<group_display_sql><![CDATA[SELECT system.id AS `system.id`, system.description AS `system.desription`, system.manufacturer AS `system.manufacturer`, system.model AS `system.model`, system.serial AS `system.serial`, system.service_number AS `system.service_number`, system.service_provider AS `system.service_provider`, system.owner AS `system.owner`, system.icon AS `system.icon`, system.os_family AS `system.os_family`, system.type AS `system.type` FROM system LEFT JOIN oa_group_sys ON (system.id = oa_group_sys.system_id) WHERE oa_group_sys.group_id = ? GROUP BY system.id]]></group_display_sql>
	</details>
