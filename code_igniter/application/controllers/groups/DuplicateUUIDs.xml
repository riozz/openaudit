<group>
	<details>
		<group_name>Duplicate UUIDs</group_name>
		<group_padded_name/>
		<group_dynamic_select><![CDATA[SELECT distinct(system.id) FROM system INNER JOIN (SELECT uuid FROM system GROUP BY uuid HAVING count(uuid)> 1) system2 ON system.uuid = system2.uuid WHERE system2.uuid != '']]></group_dynamic_select>
		<group_parent>1</group_parent>
		<group_description>Any items in the system table that have an identical UUID entry regardless of status.</group_description>
		<group_category>general</group_category>
		<group_display_sql><![CDATA[SELECT system.id AS `system.id`, system.icon AS `system.icon`, system.name AS `system.name`, system.uuid AS `system.uuid`, system.status AS `system.status`, system.type AS `system.type`, system.os_family AS `system.os_family`, system.os_name AS `system.os_name`, system.last_seen AS `system.last_seen` FROM system, oa_group_sys WHERE system.id = oa_group_sys.system_id AND oa_group_sys.group_id = ? GROUP BY system.id]]></group_display_sql>
		<group_icon>general</group_icon>
	</details>
